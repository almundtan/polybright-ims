dev:
	npm run dev

dev-api:
	npm run dev:api

dev-web:
	npm run dev:web

dev-mobile:
	npm run dev:mobile

install:
	npm install

fmt:
	npm run fmt

lint:
	npm run lint

typecheck:
	npm run typecheck

test:
	npm test

migrate:
	npm run migrate

db-seed:
	npm run db:seed

build:
	npm run build

start-docker:
	npm run docker:up
