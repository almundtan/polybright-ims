{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "rootDir": "src",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "@/config/*": ["config/*"],
      "@/controllers/*": ["controllers/*"],
      "@/middlewares/*": ["middlewares/*"],
      "@/routes/*": ["routes/*"],
      "@/services/*": ["services/*"],
      "@/repositories/*": ["repositories/*"],
      "@/utils/*": ["utils/*"],
      "@/types/*": ["types/*"],
      "@/modules/*": ["modules/*"]
    }
  },
  "include": ["src"],
  "references": []
}
